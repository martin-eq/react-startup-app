{
  "name": "react-startup-app",
  "version": "0.0.1",
  "description": "React startup app",
  "private": true,
  "homepage": "https://github.com/martin-eq/react-startup-app#readme",
  "scripts": {
    "start": "npm run bundle-prod && npm run prod",
    "dev": "yarn && node server/start.js",
    "prod": "yarn && NODE_ENV=production node server/start.js",
    "debug": "node --inspect --debug-brk server/start.js",
    "bundle": "webpack --config webpack/webpack.config.dev.js",
    "bundle-prod": "webpack --config webpack/webpack.config.prod.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "flow": "flow || true",
    "lint": "eslint --ext .jsx --ext .js . || true",
    "flow-typed": "flow-typed install"
  },
  "dependencies": {
    "express": "^4.15.3",
    "react": "^15.5.4",
    "react-apollo": "^1.4.2",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.47.0",
    "flow-typed": "^2.1.2",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "^6.0.1",
    "react-flow-types": "^0.1.1",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-hot-server-middleware": "^0.1.0",
    "react-hot-loader": "next"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martin-eq/react-startup-app.git"
  },
  "bugs": {
    "url": "https://github.com/martin-eq/react-startup-app/issues"
  },
  "author": "Martin Quintana <quintanamartine@gmail.com>",
  "license": "ISC",
  "keywords": [
    "react",
    "startup",
    "graphql",
    "apollo",
    "webpack",
    "babel",
    "es6"
  ]
}
